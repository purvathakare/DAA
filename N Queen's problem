#include <stdio.h>
#include <stdbool.h>
int count = 0;
int board[10];
bool is_safe(int row, int col, int n){
	for(int i = 0; i < row; i++){
		if(board[i] == col || board[i]-i == col-row || board[i]+i == col+row){
			return false;
		}
	}
	return true;
}
void solve(int row, int n){
	if(row == n){
		count++;
		return;
	}
	for(int col = 0; col < n; col++){
		if(is_safe(row, col , n)){
			board[row] = col;
			solve(row+1, n);
		}
	}
}
int nQueen(int n){
	count = 0;
	solve(0, n);
	return count;
}
int main(){
	int n;
	scanf("%d", &n);
	printf("%d\n", nQueen(n));
	return 0;
}
